{
  "version": 2,
  "regions": ["lhr1"],
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/stock-check",
      "dest": "/api/stock_check.py"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.py"
    },
    {
      "src": "/stock-check",
      "dest": "/api/stock_check.py"
    },
    {
      "src": "/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/webhook",
      "dest": "/api/webhook.py"
    }
  ]
}

